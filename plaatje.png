    /**
     * De functie rate(bid, type) in de globale Lexical Scope
     * zorgt voor het kunnen stemmen op commentaren en artikelen
     * Deze functie wordt niet in dit bestand aangeroepen,
     * maar wel in rate_reply.php; rate_artikel.php.
     * Voorbeelden:
     *	bid = artikel[number]; type = article.
     *   bid = artikel[number]; type = reply.
     * 
     */
    function rate(object_id, object_type) {

        var rate_form = $('#' + object_id).parents('form');
        var rate_result = $('#r' + object_id).parents('form');
        var datastring = rate_form.serialize();

        // Hier wordt een formulier gestuurd naar /rate
        $.ajax({
            type: "POST",
            url: "/rate",
            data: datastring,
            // het formulier is succesvol gestuurd, er is data...
            success: function (data) {
                data = JSON.parse(data);
                var newresult = data.average;
                var newroundresult = Math.round(data.average * 2);
                if (data.status == 'success') {
                    if (object_type == 'reply') {
                        $("div#vote" + object_id).unbind('mouseenter');
                        rate_form.html("");
                        rate_result.show();
                        $('#article_result_form  .star').rating('readOnly', true);
                        $('#reply_comment' + object_id).html("");
                        $('#reply_result_comment' + object_id).html("<span style='color: #ee1122'>"+data.msg+"</span>");
                        $('.star' + object_id + '[value="' + newresult + '"]').attr('checked', true);
                        $('.star' + object_id).rating('select', '' + newresult);
                        $('#votef'+ object_id+' .star').rating('readOnly', true);
                        setTimeout(function () {
                            $('div#voted' + object_id).mouseenter(function () {
                                $('#reply_result_comment' + object_id).html("<span style='color: #ee1122'>U hebt deze reactie al beoordeeld</span>");
                                $('#reply_comment' + object_id).html("");
                            });
                        }, 3000);
                    } else {
                        $("div.hover-result").unbind('mouseenter');
                        rate_form.html("");
                        rate_result.show();
                        
                        if (newresult == "") {
                            $('#resultstring')
                                .mouseenter(function () {
                                    $("#article_result_msg_hover").html("Respons nog te laag voor een beoordeling");
                                })
                                .mouseleave(function () {
                                    $("#article_result_msg_hover").html('<span  class="artratebeo">Beoordeling:</span>');
                                });
                        } else {
                       // $('#resultstring').html(newresult.replace('.', ','));
                            $('#article_result_form  .star').rating('select', '' + newroundresult);
                            $('#article_result_form  .star').rating('readOnly', true);
                            $('#article_result_msg').show();
                            $('#article_result_msg').html(data.msg);
                            $('#article_result_msg_hover').html(data.msg);
                        }
                        $('div.hover-result')
                            .mouseleave(function () {
                                $("#article_result_msg_hover").html('<span  class="artratebeo">Beoordeling:</span>');
                            });
                        setTimeout(function () {
                            $('div.hover-result')
                                .mouseenter(function () {
                                    $("#article_result_msg_hover").html("U hebt dit artikel al beoordeeld");
                                });
                        }, 3000);
                    }
                } else {
                    if (object_type == 'reply') {
                        $('#reply_result_comment' + object_id).html("<span style='color: #ee1122'>"+data.msg+"</span>");
                    } else{
                        $("#article_result_msg").html(data.msg);
                    }
                }
            },
            error: function () {
                rate_form.html("Het versturen is helaas mislukt");
            }
        });
    }

