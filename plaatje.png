                var scrollTop = $(window).scrollTop();
                if (scrollTop > stickyShareTop) {
                    $('.social').addClass('stickyshare');
                } else {
                    $('.social').removeClass('stickyshare');
                }
            };
            if (!is_safari) {
                stickyShare();
            }
            $(window).scroll(function () {
                if (!is_safari) {
                    stickyShare();
                }
            });
        }
    });
    
    function switchSearch() {
        if ($('#zoekwaar').val() == 'artikelen') {
            $('#zoekform').attr('action', 'https://www.computable.nl/zoek');
        }
        if ($('#zoekwaar').val() == 'whitepapers') {
            $('#zoekform').attr('action', '//whitepapers.computable.nl/');
        }
    }
    
    $("#UserLogin").mouseout(function () {
        $('#logindropdownul').show();
	});
	
	$('#loginknop').off('click');

    var loginState = 0;
    function toggleLogin() {
        if ($('#logindropdown').is(':hidden')) {
            $('#logindropdown').show();
        }
        if (loginState % 2 > 0) {
            $('#logindropdown').hide();
        }
        loginState++;
    }
</script></body>
</html>